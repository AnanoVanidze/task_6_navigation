<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".BottomSheetFragment">


    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/audioTv"
        android:drawablePadding="10sp"
        android:drawableStart="@drawable/ic_baseline_keyboard_voice_24"
        android:paddingStart="16sp"
        android:textColor="@color/black"
        android:textSize="26sp"
        android:text="audio"
        app:layout_constraintBottom_toTopOf="@+id/cameraTv"
        android:layout_marginBottom="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/cameraTv"
        android:drawablePadding="10sp"
        android:drawableStart="@drawable/ic_baseline_camera_alt_24"
        android:paddingStart="16sp"
        android:textColor="@color/black"
        android:textSize="26sp"
        android:text="camera"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>



</androidx.constraintlayout.widget.ConstraintLayout>